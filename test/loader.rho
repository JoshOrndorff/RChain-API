new loadedCh, trace(`rho:io:stderr`) in {
  __TOP__
    new uriCh, insertArbitrary(`rho:registry:insertArbitrary`) in {
      for (@it <- __EXPORT__) {
        insertArbitrary!(it, *uriCh) |
        for (@uri <- uriCh) {
          trace!({"__NAME__ uri": uri}) |
          loadedCh!(uri)
        }
      }
    } |

    __BODY__
  }
}
